      <dependency id="structuremap" version="2.6.4.1" />
      <dependency id="Microsoft.AspNet.WebApi" version="4.0.20710.0" />
    </dependencies>
  </metadata> 
  <files>
      <file src="content\**\*.pp" target="content" />
